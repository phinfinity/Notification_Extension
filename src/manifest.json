{
  "name": "Notification Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A unified Central Notifications system",
  "background": { "page": "background.html" },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "",
      "default_icon": "icon.png",
      "name": "",
      "default_popup":"popup.html"
  },
  "sandbox": {
	  "pages": ["sandbox.html"]
  },
  "options_page":"options.html"
}
